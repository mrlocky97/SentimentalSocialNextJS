{
  "name": "sentimentalsocial",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx tsc --project tsconfig.server.json && node dist/server.js",
    "build": "npx tsc --project tsconfig.server.json",
    "build:server": ".\\scripts\\build\\start-server.bat",
    "start": "node dist/server.js",
    "debug": "nodemon --inspect=0.0.0.0:9229 --exec ts-node --project tsconfig.server.json src/server.ts",
    "debug:break": "nodemon --inspect-brk=0.0.0.0:9229 --exec ts-node --project tsconfig.server.json src/server.ts",
    "debug:prod": "node --inspect dist/server.js",
    "train": "tsx scripts/train-model-kfold.ts",
    "train:cv": "cross-env EVAL_MODE=CROSS_VALIDATION KFOLD=5 tsx scripts/train-model-kfold.ts",
    "train:cv10": "cross-env EVAL_MODE=CROSS_VALIDATION KFOLD=10 tsx scripts/train-model-kfold.ts",
    "train:split": "cross-env EVAL_MODE=SINGLE_SPLIT tsx scripts/train-model-kfold.ts",
    "test": "jest --config config/jest.config.js",
    "test:watch": "jest --config config/jest.config.js --watch",
    "test:coverage": "jest --config config/jest.config.js --coverage",
    "test:unit": "jest --config config/jest.config.js --testPathPatterns=tests/services",
    "test:integration": "jest --config config/jest.config.js --testPathPatterns=tests/api",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --config config/jest.config.js --runInBand",
    "test:ci": "jest --config config/jest.config.js --coverage --watchAll=false --ci",
    "auth": "tsx src/utils/twitter-auth-helper.ts",
    "cookie-helper": "tsx src/utils/cookie-importer.ts",
    "encrypt-twitter-creds": "tsx scripts/encrypt-twitter-creds.ts",
    "ml:update-hybrid": "tsx src/scripts/update-hybrid-system.ts",
    "test:essential": "tsx src/scripts/essential-test.ts",
    "test:basic": "tsx src/scripts/test-basic.ts",
    "sentiment:train": "tsx src/scripts/train-sentiment-model.ts",
    "sentiment:evaluate": "tsx src/scripts/evaluate-accuracy.ts",
    "eval": "node scripts/run-evaluation.js",
    "eval:simple": "node scripts/run-evaluation.js simple",
    "eval:slang": "node scripts/run-evaluation.js slang",
    "eval:all": "node scripts/run-evaluation.js all",
    "eval:help": "node scripts/run-evaluation.js help",
    "eval:unified": "tsx src/scripts/evaluate-unified-sentiment-system.ts",
    "cross-validation": "node scripts/simple-cross-validation.js",
    "test:slang": "node scripts/test-enhanced-system.js",
    "sentiment:save": "tsx src/scripts/save-model.ts",
    "test:reactive": "tsx src/scripts/test-reactive.ts",
    "test:advanced": "tsx src/scripts/test-advanced.ts",
    "test:complete": "tsx src/scripts/test-complete.ts",
    "test:module": "tsx src/scripts/test-complete.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=moderate",
    "clean": "if exist dist rmdir /s /q dist && if exist coverage rmdir /s /q coverage && if exist lint-results.json del lint-results.json && if exist audit-results.json del audit-results.json",
    "clean:windows": "if exist dist rmdir /s /q dist && if exist coverage rmdir /s /q coverage && if exist lint-results.json del lint-results.json && if exist audit-results.json del audit-results.json",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully!'",
    "ci:all": "npm run lint && npm run type-check && npm run test:ci && npm run build"
  },
  "dependencies": {
    "@crawlee/core": "^3.13.10",
    "@crawlee/playwright": "^3.13.10",
    "@huggingface/inference": "^2.6.1",
    "@tensorflow/tfjs": "^4.16.0",
    "@the-convocation/twitter-scraper": "^0.17.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.3",
    "morgan": "^1.10.0",
    "natural": "^8.1.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "playwright": "^1.54.1",
    "puppeteer": "^24.12.1",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/next": "^8.0.7",
    "@types/node": "^20",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0"
  }
}
